<nav>
  <% if session[:id] %>
    <div>
      <%= link_to("Signout", session_path(session[:id]), :method => :delete) %>
    </div>
  <% else %>  
    <div>
      <%= link_to("Login", new_session_path) %>
    </div>
    <div>
      <%= link_to("Sign Up", new_user_path) %>
    </div>
  <% end %>
</nav>